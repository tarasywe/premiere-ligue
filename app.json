{
  "expo": {
    "name": "premier-league",
    "slug": "premier-league",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "premier-league",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0891b2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tmakota.wiki"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0891b2",
          "resizeMode": "contain",
          "imageWidth": 200,
          "android": {
            "resizeMode": "contain",
            "imageWidth": 200,
            "backgroundColor": "#0891b2",
            "backgroundImage": "./assets/images/icon.png",
            "image": "./assets/images/icon.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.tmakota.wiki",
      "googleServicesFile": "./google-services.json"
    }
  }
}